#include <stdio.h>
int main(){
    int a[5],b[10],c[10],i;

        for(i=0;i<5;i++){
            printf("Digite os valor de A(%d)", i);
            scanf("%d", &a[i]);
    }
        for(i=5;i<10;i++){
            printf("Digite os valor de B(%d)", i);
            scanf("%d", &b[i]);
    }
        for(i=0;i<5;i++){
            c[i] = a[i];
    }
        for(i=5;i<10;i++){
            c[i] =  b[i];
    }
        for(i=0;i<10;i++){
            printf("\nO valor de C eh: %d\n", c[i]);
    }

return 0;
}
